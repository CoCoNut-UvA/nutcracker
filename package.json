{
  "name": "nutcracker",
  "displayName": "NutCracker",
  "description": "Syntax highlighting for CoCoNut DSL",
  "publisher": "CoCoNut-UvA",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/CoCoNut-UvA/nutcracker"
  },
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ccn",
      "aliases": ["CoCoNut DSL", "ccn"],
      "extensions": [".ccn"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ccn",
      "scopeName": "source.ccn",
      "path": "./syntaxes/ccn.tmLanguage.json"
    }]
  }
}
